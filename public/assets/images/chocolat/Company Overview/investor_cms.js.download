function contactusDetails(id) {
	var info_div = id+"_info";
	$("#"+info_div).toggle();
	if($("#"+info_div).is(':visible')) {
		$("#"+id).addClass('active');
	} else {
		$("#"+id).removeClass('active');
	}
	if($("#"+info_div).is(':visible')) {	
		$("html, body").animate({
			scrollTop: $("#"+info_div).offset().top-300
		}, 100);
	}
}







