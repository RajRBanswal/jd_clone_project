function media_details(name, id, sel_tab='', restvalues ={}) {
	const obj = Object.assign({isScroll: true}, restvalues)
	if (obj.isScroll) {
		if((name == 'Press' && sel_tab == '') || (name == 'consumer_report' && sel_tab == '') || (name == 'video' && sel_tab == 'media_all') || detectDevice == 'mobile') {
			$("html, body").animate({
				scrollTop: $(".mpad_section").offset().top-20
			}, 100);
		}
		if(name == 'video' && sel_tab == '' &&  detectDevice == 'desktop') {
			$("html, body").animate({
				scrollTop:   $("#vid_"+id).offset().top - 100
			}, 2000);
		}
	}

	$.ajax({
		url: CMSDOMAIN+'webcms_new/functions/cms/ajax_cms.php',
		type: 'POST',
		data: {
			action: 'showMediaArticle',
			page : page,
			async: false,
			hname : id,
			type : name
		},
		success: function(response){
			response = JSON.parse(response);
			if(name == 'video' && sel_tab == '' ) { //This is for video tab. video will open in left popupbox
				$(".videocontainer").removeClass('active');
				$("#vid_"+id).addClass('active');
				$("#video_play").html('');
				var videos_link = response['results']['videos_attached'].split('//');

				$("#video_play").html('<div class="mgimgbox"><div class="ytframe-wrap"><iframe title="video link" width="100%" height="100%" frameborder="0" allowfullscreen="" src="https://'+videos_link[1]+'?autoplay=1&mute=1" allow="autoplay"></iframe><div id="YTPlayer" class="yt-iframe"></div></div></div><div class="mgtext_1">'+response['results']['created_date']+'</div><div class="mgtext_2">'+response['results']['articlename']+'</div>');
			} else { // This is for media all and press tab. vedio and press will open in popupbox
				$('#media_details').removeClass('dn');
				$(".tab").addClass('dn');
				if(sel_tab == 'media_all' && name == 'video') {
					var videos_link = response['results']['videos_attached'].split('//');
					$("#media_details").html('<div class="mgimgbox" style="background-color:#fff"><div class="imageheading font18 fw500 mb-20">'+response['results']['articlename']+'<span class="imageclose_icon" onclick="closetab(\''+name+'\');"></span></div><div class="ytframe-wrap"><iframe title="video link" width="100%" height="100%" frameborder="0" allowfullscreen="" src="https://'+videos_link[1]+'" id="iframevideo"></iframe><div id="YTPlayer" class="yt-iframe"></div></div></div>');
				} else {
					$("#media_details").html('<div class="mdbox"><div class="imageheading font18 fw500 mb-20">'+response['results']['articlename']+'<span class="imageclose_icon" onclick="closetab();"></span></div><img style="width:80%" src="'+response['results']['imagesattached']+'"><img></div></div>');
				}
			}
		}
	})
}

