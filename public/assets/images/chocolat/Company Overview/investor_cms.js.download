					redirection_client_url = CMSDOMAIN+'cms/'+search_url+'/'+art_publishername+'/'+art_articlename+'/id-'+entry['hid']+'?jdlite='+JDLITEQUERY;

					if(entry['pid'] == '25' ) {
						redirection_client_url = CMSDOMAIN+'cms/'+search_url+'/'+art_articlename+'/id-'+entry['hid']+'?jdlite='+JDLITEQUERY;
					}
					fill_text += '<div class="dtbl mdbox_row"><div class="dcell_top crpcolum_img">';
					if(entry['consumer_report_flag'] == 1 && entry['imageattached'] == '' && entry['pid'] == '11') { 
						fill_text +='<div class="contentimg" onClick="downlaodCMSPDF("'+entry['pdfattached']+'")">'; 
					} else {
						fill_text += '<div class="contentimg"><a href="'+redirection_client_url+'">';
					}
					fill_text +='<img class="" src="'+img+'"><i class="'+className+'"></i></a></div></div><div class="dcell_top crpcolum_info mdcolum_info"><div class="crpcolum_1">'+created_date+'</div>';
					if(entry['consumer_report_flag'] == 1 && entry['imageattached'] == '' && entry['pid'] == '11') { 
						fill_text +='<div class="crpcolum_2" onclick="downlaodCMSPDF("'+entry['pdfattached']+'");">'+articlename+'</div>';
					} else { 
						fill_text +='<div class="crpcolum_2"><a href="'+redirection_client_url+'">'+articlename+'</a></div>';
					}
					fill_text +='<div class="crpcolum_3 mbcolum">'+body+'</div>';
					if(entry['pdfattached'] != '' && entry['pdfattached'] != null) { 
						fill_text +='<div class="reporig" onClick="downlaodCMSPDF("'+entry['pdfattached']+'")"> <span class="downlimgc"></span>Download</div>';
					} 
					fill_text +='</div></div>';
				});
				if(name == 'Media') {
					$('#media_all_data').append(fill_text);
				} else if(name == 'Press') {
					$('#press_data').append(fill_text);
				} else if(name == 'consumer_report') {
					$('#consumer_report_data').append(fill_text);
				}
				if(response['result']['numPages'] == page || response['result']['numPages'] == 0) {
					$('#'+name.toLowerCase()+'_showmore_anchr').addClass('disabled');
					$('#'+name.toLowerCase()+'_showmore_anchr').prop("onclick", null).off("showMoreVideo");
				}
			}
		}
	})
}
